{
  "ConnectionStrings": {
    "Local": "Server=.;Database=Uno;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FileMaxSize": 5,
  "FileSupportedFormats": {
    "VideoFormats": [

      ".mp4",
      ".mov",
      ".avi",
      ".webm"
    ]
  },
  "HostConfig": {
    "BaseProject": "UNo.Presentation",
    "BaseDirectory": "wwwroot",
    "BaseFolder": "Host"
  },
  "HostedFilesJobConfig": {
    "TakeCount": 10,
    "Period": 1 // min 
  },
  "SendIssueJobConfig": {
    "TakeCount": 5,
    "TimePeriod": 30, // 2 min 
    "TryCountAmount": 2
  },
  "AllowedHosts": "*"
}
